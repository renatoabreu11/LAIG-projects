         <primitive id="bottomMountain">
            <patch orderU="2" orderV="5" partsU="3" partsV="15" >
                <controlpoint x="-0.048" y="-0.491" z="-0.000" />
                <controlpoint x="-0.048" y="-0.491" z="-0.000" />
                <controlpoint x="-0.048" y="-0.491" z="0.000" />
                <controlpoint x="-0.048" y="-0.491" z="-0.000" />
                <controlpoint x="0.048"  y="-0.491" z="-0.000" />
                <controlpoint x="-0.048" y="-0.491" z="-0.000" />

                <controlpoint x="0.000" y="-1.000" z="0.400" />
