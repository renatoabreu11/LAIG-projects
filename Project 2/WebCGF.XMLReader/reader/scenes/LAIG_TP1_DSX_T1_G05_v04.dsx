<dsx>
    <scene root="root" axis_length="30.0" />
    
    <views default="front" >
        <perspective id="front" near="0.1" far="500.0" angle="60.0">
            <from x="45" y="45" z="45.0" />
            <to x="0" y="0" z="0" />
        </perspective>

        <perspective id="back" near="0.2" far="500.0" angle="60.0">
            <from x="-45" y="45" z="-45.0" />
            <to x="0" y="0" z="0" />
        </perspective>

        <perspective id="side" near="0.2" far="500.0" angle="60.0">
            <from x="5" y="30.0" z="30.0" />
            <to x="0" y="0" z="0" />
        </perspective>

        <perspective id="other" near="0.2" far="500.0" angle="90.0">
            <from x="30" y="25.0" z="4.0" />
            <to x="0" y="0" z="0" />
        </perspective>  
    </views>

    <illumination doublesided="1" local="0" >
        <ambient r="0.5" g="1" b="0.5" a="0.5" />
        <background r="0" g="0" b="0" a="1" />
    </illumination>
    
    <lights>
        <omni id="light1" enabled="1" >
            <location x="-17.75" y="12.2" z="9" w="1" />
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="1" g="1" b="1" a="1" />
            <specular r="0" g="0" b="0" a="1" />
        </omni>

        <omni id="light2" enabled="1" >
            <location x="12" y="16" z="-17.85" w="1" />
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="1" g="1" b="1" a="1" />
            <specular r="0" g="0" b="0" a="1" />
        </omni>
        
        <spot id="light3" enabled="1" angle="60" exponent="1">
            <target x="0" y="7.5" z="0" />
            <location x="-10.5" y="18" z="-12.5"/>
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="1" g="1" b="1" a="1" />
            <specular r="0" g="0" b="0" a="1" />
        </spot>

        <spot id="light4" enabled="1" angle="60" exponent="1">
            <target x="15" y="0" z="15" />
            <location x="15" y="45" z="15"/>
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="1" g="1" b="1" a="1" />
            <specular r="0" g="0" b="0" a="1" />
        </spot>
    </lights>
    
    <textures>
        <texture id="grass" file="../res/grass.jpg" length_s="2" length_t="1" />
        <texture id="skin" file="../res/skin.jpg" length_s="1" length_t="1" />
        <texture id="sand" file="../res/sand.jpg" length_s="20" length_t="20" />
        <texture id="sand2" file="../res/sand2.jpg" length_s="20" length_t="20" />
        <texture id="troll" file="../res/troll.jpg" length_s="1" length_t="1" />
        <texture id="solrock" file="../res/solrock.jpg" length_s="1" length_t="1" />
        <texture id="bluefire" file="../res/bluefire.jpg" length_s="1" length_t="1" />
        <texture id="eye" file="../res/eye.png" length_s="1" length_t="1" />
        <texture id="onix" file="../res/onix.jpg" length_s="1" length_t="1" />
        <texture id="white" file="../res/white.jpg" length_s="1" length_t="1" />
        <texture id="grey" file="../res/grey.jpg" length_s="1" length_t="1" />
        <texture id="black" file="../res/black.jpg" length_s="1" length_t="1" />
        <texture id="wood" file="../res/wood.jpg" length_s="1" length_t="1" />
        <texture id="left" file="../res/left.jpg" length_s="1" length_t="1" />
        <texture id="back" file="../res/back.jpg" length_s="1" length_t="1" />
        <texture id="front" file="../res/front.jpg" length_s="1" length_t="1" />
        <texture id="right" file="../res/right.jpg" length_s="1" length_t="1" />
        <texture id="bottom" file="../res/bottom.jpg" length_s="1" length_t="1" />
        <texture id="top" file="../res/top.jpg" length_s="1" length_t="1" />
        <texture id="ice" file="../res/ice.jpg" length_s="1" length_t="1" />
    </textures>

    <materials>
        <material id="s1" >
            <emission r="0.3" g="0.4" b="0.5" a="1" />
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="0.3" g="0.2" b="0.6" a="1" />
            <specular r="0.2" g="0.2" b="0.2" a="1" />
            <shininess value="120" />
        </material>

        <material id="s2" >
            <emission r="0.3" g="0.4" b="0.5" a="1" />
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="0.9" g="0.9" b="0.6" a="1" />
            <specular r="0.1" g="0.1" b="0.4" a="1" />
            <shininess value="1" />
        </material>

        <material id="yellow" >
            <emission r="0.1" g="0.4" b="0.5" a="1" />
            <ambient r="1" g="0.843137" b="0" a="1" />
            <diffuse r="0.7" g="0.843137" b="0" a="1" />
            <specular r="0.1" g="0.1" b="0.4" a="1" />
            <shininess value="1" />
        </material>

        <material id="shirt" >
           <emission r="0.1" g="0.4" b="0.5" a="1" />
           <ambient r="0.117647" g="0.564706" b="1" a="1" />
           <diffuse r="0.1" g="0.1" b="0" a="1" />
           <specular r="0.1" g="0.1" b="0.1" a="1" />
           <shininess value="1" />
       </material>

       <material id="bright" >
           <emission r=".5" g=".5" b="0" a="1" />
           <ambient r="0.1" g="0.5" b="0.5" a="1" />
           <diffuse r="0.1" g="0.4" b="0.4" a="1" />
           <specular r="0.1" g="0.2" b="0.2" a="1" />
           <shininess value="1" />
       </material>

       <material id="superbright" >
           <emission r="1" g="1" b="1" a="1" />
           <ambient r="0.5" g="0.5" b="0.5" a="1" />
           <diffuse r="0.5" g="0.5" b="0.5" a="1" />
           <specular r="0.5" g="0.5" b="0.5" a="1" />
           <shininess value="1" />
       </material>

   </materials>

   <transformations>
        <transformation id="transf">
            <translate x="0" y="0" z="0" />
            <rotate axis="x" angle="90" />
            <scale x="1" y="1" z="1" />
        </transformation>
    </transformations>

   <animations>
        <animation id="anim1" span="3" type="linear">
            <controlpoint x="0" y="0" z="0" />
            <controlpoint x="18" y="0" z="0" />
        </animation>

        <animation id="enterPortal" span="8" type="linear">
            <controlpoint x="0" y="0" z="0" />
            <controlpoint x="40" y="0" z="0" />
            <controlpoint x="40" y="15" z="15" />
            <controlpoint x="41" y="25" z="25" />
        </animation>

        <animation id="reconScene" span="4" type="circular" centerx="-60" centery="0" centerz="0"
                    radius="60" startang="0" rotang="145"/>

        <animation id="preparation" span="8" type="linear">
            <controlpoint x="0" y="0" z="0" />
            <controlpoint x="40" y="-80" z="75" />
             <controlpoint x="-75" y="-75" z="75" />
             <controlpoint x="-75" y="-30" z="75" />
             <controlpoint x="-30" y="-30" z="75" />
        </animation>

        <animation id="exitPortal" span="4" type="linear">
             <controlpoint x="0" y="0" z="0" />
             <controlpoint x="120" y="5" z="-35" />
        </animation>

        <animation id="pause" span="20" type="linear">
            <controlpoint x="0" y="0" z="0" />
            <controlpoint x="0" y="0.001" z="0" />
        </animation>

        <animation id="shortPause" span="4" type="linear">
            <controlpoint x="0" y="0" z="0" />
            <controlpoint x="0" y="0.001" z="0" />
        </animation>

        <animation id="abduction" span="4" type="linear">
            <controlpoint x="0" y="0" z="0" />
            <controlpoint x="0" y="10" z="0" />
        </animation>

        <animation id="takenAway" span="4" type="linear">
                    <controlpoint x="0" y="0" z="0" />
                    <controlpoint x="30" y="5" z="-120" />
        </animation>

        <animation id="rotate45" span="3" type="circular" centerx="0" centery="0" centerz="0"
            radius="3" startang="0" rotang="60"/>
   </animations>

    <primitives>
    	<primitive id="rect">
    		<rectangle x1="10" y1="10" x2="-10" y2="-10" />
    	</primitive>

    	<primitive id="onixHorn1">
    		<triangle x1="-1" y1="0" z1="0" x2="0" y2="3" z2="0" x3="1" y3="0" z3="0" />
    	</primitive>

    	<primitive id="onixHorn2">
    		<triangle x1="1" y1="0" z1="0" x2="0" y2="3" z2="0" x3="-1" y3="0" z3="0" />
    	</primitive>

    	<primitive id="onixEye">
    		<rectangle x1="0" y1="0" x2="1" y2=".5" />
    	</primitive>

       <primitive id="pyramidWall">
            <triangle x1="-10" y1="0" z1="-10" x2="-10" y2="0" z2="10" x3="0" y3="15" z3="0" />
        </primitive>

        <primitive id="cyl">
            <cylinder base="1" top="1" height="1" slices="8" stacks="8" />
        </primitive>

        <primitive id="sper">
            <sphere radius=".5" slices="8" stacks="8" />
        </primitive>

        <primitive id="boulder">
            <sphere radius="1" slices="8" stacks="8" />
        </primitive>

        <primitive id="tor">
            <torus inner="1" outer="4" slices="12" loops="12" />
        </primitive>

        <primitive id="micPart">
            <cylinder base=".25" top=".25" height="2" slices="8" stacks="8" />
        </primitive>

        <primitive id="rai">
            <cylinder base=".2" top="0.03" height="1" slices="8" stacks="8" />
        </primitive>

        <primitive id="plane">
         <plane dimX="1" dimY="1" partsX="8" partsY="8" >
         </plane>
        </primitive>

        <primitive id="chessboard">
        <chessboard du="8" dv="8" textureref="wood" su="1" sv="1">
                   <c1 r="0.2" g="0.3" b="0.4" a="1" />
                   <c2 r="1" g="1" b=".5" a="1" />
                   <cs r="1" g="0" b="0" a="1" />
        </chessboard>
        </primitive>

        <primitive id="smallBoard">
        <chessboard du="2" dv="2" textureref="skin" su="-1" sv="-1">
                   <c1 r="0.2" g="0.3" b="0.4" a="1" />
                   <c2 r="1" g="1" b=".5" a="1" />
                   <cs r="1" g="0" b="0" a="1" />
        </chessboard>
        </primitive>

        <primitive id="mediumBoard">
        <chessboard du="4" dv="4" textureref="skin" su="-1" sv="-1">
                   <c1 r="0.2" g="0.3" b="0.4" a="1" />
                   <c2 r="1" g="1" b=".5" a="1" />
                   <cs r="1" g="0" b="0" a="1" />
        </chessboard>
        </primitive>

         <primitive id="patch">
