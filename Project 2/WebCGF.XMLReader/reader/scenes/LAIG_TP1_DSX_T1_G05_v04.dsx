         <primitive id="spaceship">
            <vehicle />
         </primitive>

          <primitive id="portal">
                <portal />
          </primitive>

    </primitives>

    <components>
        <component id="root">
            <transformation>
            </transformation>

            <materials>
                <material id="s1" />
                <material id="s2" />
            </materials>

            <texture id="none" />

            <children>
                <componentref id="mountain" />
                <componentref id="spaceship" />
                <componentref id="ground" />
                <componentref id="pyramids" />
                <componentref id="skybox"/>
                <componentref id="persons" />
                <componentref id="onix" />
                <componentref id="solrock" />
                <componentref id="bottomPortal" />
                <componentref id="topPortal" />
                <componentref id="chessboard" />
                <componentref id="3Dboard" />
            </children>
        </component>

        <component id="3Dboard">
            <transformation>
                <translate x="0" y="15" z="0" />
            </transformation>

            <materials>
                <material id="s1" />
            </materials>

            <texture id="none" />

            <children>
                <componentref id="topBoard" />
                <componentref id="bottomBoard" />
                <componentref id="centerBoard" />
             </children>
        </component>

        <component id="topBoard">
            <transformation>
                <translate x="-3" y="6" z="-3" />
                <rotate axis="x" angle="-90"/>
            </transformation>

            <materials>
                <material id="s1" />
            </materials>

            <texture id="none" />

            <children>
                <primitiveref id="mediumBoard" />
             </children>
        </component>

        <component id="centerBoard">
            <transformation>
                <translate x="0" y="4" z="0" />
                <rotate axis="x" angle="-90"/>
            </transformation>

            <materials>
                <material id="s1" />
            </materials>

            <texture id="none" />

            <children>
                <primitiveref id="mediumBoard" />
             </children>
        </component>

        <component id="bottomBoard">
            <transformation>
                <translate x="3" y="2" z="3" />
                <rotate axis="x" angle="-90"/>
            </transformation>

            <materials>
                <material id="s1" />
            </materials>

            <texture id="none" />

            <children>
                <primitiveref id="mediumBoard" />
             </children>
        </component>

        <component id="persons">
            <transformation>
                <translate x="0" y="0" z="0" />
            </transformation>
            <materials>
                <material id="s1" />
                <material id="s2" />
            </materials>

            <texture id="none" />

            <children>
                <componentref id="person1" />
                <componentref id="person2" />
             </children>
        </component>

        <component id="person1">
            <transformation>
                <translate x="0" y="0" z="0" />
            </transformation>
            <animation>
                <animationref id="pause"/>
                <animationref id="abduction"/>
                <animationref id="takenAway"/>
            </animation>

            <materials>
                    <material id="superbright" />
            </materials>

            <texture id="none" />

            <children>
                <componentref id="body" />
             </children>
        </component>

        <component id="person2">
            <transformation>
                <translate x="15" y="0" z="15" />
                <rotate axis="y" angle="180"/>
            </transformation>

            <materials>
                    <material id="superbright" />
            </materials>

            <texture id="none" />

            <children>
                <componentref id="body" />
             </children>
        </component>

        <component id="chessboard">
            <transformation>
                <translate x="7.5" y="3.5" z="7.5" />
                <rotate axis="x" angle="-90"/>
            </transformation>

            <materials>
                    <material id="superbright" />
            </materials>

            <texture id="none" />

            <children>
                <primitiveref id="chessboard" />
             </children>
        </component>


        <component id="spaceship">
            <transformation>
                <translate x="-115" y="25" z="25" />
            </transformation>

            <animation>
                 <animationref id="enterPortal"/>
                 <animationref id="reconScene"/>
                 <animationref id="preparation"/>
                 <animationref id="shortPause"/>
                 <animationref id="exitPortal"/>
            </animation>

            <materials>
                    <material id="superbright" />
            </materials>

            <texture id="none" />

            <children>
                <primitiveref id="spaceship" />
             </children>
        </component>

        <component id="mountain">
            <transformation>
                <translate x="45" y="-45" z="40" />
                <scale x="5" y="5" z="5" />
                <rotate axis="z" angle="180"/>
            </transformation>

            <materials>
                <material id="inherit" />
            </materials>

            <texture id="ice" />

            <children>
                <primitiveref id="mountain" />
                <componentref id="mountainBottom" />
            </children>
