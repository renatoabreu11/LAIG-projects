				<translate x="4.0" y="0.0" z="4.0" />
				<rotate axis="x" angle="-90" />
			</transformation>

			<materials>
				<material id="material1" />
			</materials>

			<texture id="inherit"/>

			<children>

				<componentref id="anjoB"/>

